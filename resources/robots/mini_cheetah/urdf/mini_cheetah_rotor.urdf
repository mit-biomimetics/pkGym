        <inertia ixx="0.000245" ixy="0" ixz="0.0" iyy="0.000248" iyz="0" izz="0.000006"/>
    </inertial>
    <visual>
        <geometry>
            <mesh filename="meshes/lower_leg.dae"/>
        </geometry>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
    </visual>
    <collision>
        <geometry>
            <cylinder length ="0.1" radius = "0.01"/>
        </geometry>
        <origin rpy="0 0 0" xyz="0 0 -0.1"/>
    </collision>
</link>

